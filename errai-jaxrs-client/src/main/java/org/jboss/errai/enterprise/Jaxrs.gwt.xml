<module>
    <inherits name="org.jboss.errai.ioc.Container"/>
    <entry-point class='org.jboss.errai.enterprise.client.jaxrs.JaxrsClientBootstrap'/>
    
    <generate-with class="org.jboss.errai.enterprise.rebind.JaxrsExtensionsGenerator">
        <when-type-assignable
