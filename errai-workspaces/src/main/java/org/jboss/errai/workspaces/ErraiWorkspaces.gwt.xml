<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 1.6//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/1.6/distro-source/core/src/gwt-module.dtd">
<module>
    <inherits name="org.jboss.errai.bus.ErraiBus"/>
    <inherits name="org.jboss.errai.widgets.ErraiWidgets" />

    <inherits name='com.allen_sauer.gwt.dnd.gwt-dnd'/>
    <inherits name="com.google.gwt.user.theme.chrome.Chrome"/>

    <generate-with class="org.jboss.errai.workspaces.rebind.WorkspaceLoaderBootstrapGenerator">
        <when-type-assignable
                class="org.jboss.errai.workspaces.client.framework.ModuleLoaderBootstrap"/>
    </generate-with>

    <!-- Specify the app entry point class.                   -->
    <entry-point class='org.jboss.errai.workspaces.client.Workspace'/>
    <stylesheet src="errai-workspaces-default.css"/>

</module>
