  "author": {
    "name": "Gianluca",
    "email": "gzuccare@redhat.com"
  },
  "keywords": [],
  "homepage": "https://github.com/kiegroup/kie-tools",
  "repository": {
    "type": "git",
    "url": "https://github.com/kiegroup/kie-tools.git"
  },
  "bugs": {
    "url": "https://github.com/kiegroup/kie-tools/issues"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:dev": "rimraf dist && tsc --build tsconfig.cjs.json && tsc --build tsconfig.esm.json",
    "build:prod": "rimraf dist && pnpm lint && pnpm test && rimraf dist && tsc --build tsconfig.json",
    "lint": "run-script-if --bool \"$(build-env linters.run)\" --then \"kie-tools--eslint ./src\"",
    "test": "run-script-if --ignore-errors \"$(build-env tests.ignoreFailures)\" --bool \"$(build-env tests.run)\" --then \"jest --silent --verbose --passWithNoTests\""
  },
  "dependencies": {
    "@kie-tools-core/patternfly-base": "workspace:*",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "uniforms": "^3.10.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@kie-tools-core/webpack-base": "workspace:*",
    "@kie-tools-scripts/build-env": "workspace:*",
    "@kie-tools/eslint": "workspace:*",
    "@kie-tools/root-env": "workspace:*",
    "@kie-tools/tsconfig": "workspace:*",
    "@patternfly/react-core": "^4.157.3",
    "@patternfly/react-icons": "^4.11.17",
    "@testing-library/react": "^11.2.6",
    "@types/enzyme": "3.10.8",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.168",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@types/simpl-schema": "1.12.2",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.0",
    "copy-webpack-plugin": "^11.0.0",
    "enzyme": "3.11.0",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "simpl-schema": "^1.12.0",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.4.2",
    "uniforms-bridge-simple-schema-2": "^3.10.2",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.7.3",
    "webpack-merge": "^5.8.0"
  }
}